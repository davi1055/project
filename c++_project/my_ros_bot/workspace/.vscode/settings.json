{
    "Codegeex.Privacy": true,
    "cmake.options.statusBarVisibility": "visible",
    "editor.fontSize": 18,
    "editor.fontFamily": "'Hack Nerd Font', 'monospace', monospace",
    "editor.codeLensFontFamily": "Hack Nerd Font",
    "debug.console.fontFamily": "Hack Nerd Font",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "terminal.integrated.fontSize": 16,
    "chat.editor.fontFamily": "Hack Nerd Font",
    "cmake.configureOnOpen": true,
    "editor.guides.bracketPairs": true,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#f80bd1",
        "editorBracketHighlight.foreground2": "#fffb00",
        "editorBracketHighlight.foreground3": "#ff0000",
        "editorBracketHighlight.foreground4": "#0edd20",
        "editorBracketHighlight.foreground5": "#0011ff",
        "editorBracketHighlight.foreground6": "#ffffff",
    },
    "explorer.confirmDragAndDrop": false,
    "cmake.generator": "",
    "editor.formatOnSave": true,
    "clang-format.fallbackStyle": "Google",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
}